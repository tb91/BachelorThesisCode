package projects.reactiveSpanner.exceptions;

public class InvalidBeaconlessSubgraphStrategyException extends
		RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -381070532270643762L;

	public InvalidBeaconlessSubgraphStrategyException()
	{
		super("The Beaconless Subgraph Strategy is invalid.");
	}
	
	public InvalidBeaconlessSubgraphStrategyException(String s)
	{
		super(s);
	}
}
